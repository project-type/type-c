

type Serializable = interface {
    fn serialize() -> string
}

type User = class(Serializable) {
    let name: string = ""
    let age: u32 = 20
}

type Bot = class(Serializable) {
    let name: string = ""
    let commands: string[] = []
}


let X: User = new User("hello", 32)
unsafe{X as Serializable}
